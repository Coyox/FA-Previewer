{
  "name": "FA Previewer",
  "version": "2.0.1",
  "description": "Full-size pop-up image previewer for submission view on Fur Affinity",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["http://*.furaffinity.net/*", "https://*.furaffinity.net/*"],
      "js": ["jquery.js", "jquery-ui.js", "jquery.hoverIntent.js", "popup.js"]
    }
  ],
  "web_accessible_resources": [
    "error.png",
    "jquery-ui.js",
    "jquery.hoverIntent.js",
    "jquery.js",
    "loading.gif",
    "popup.js"
  ]
}
